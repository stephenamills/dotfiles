# brew "oauth2l" # Simple CLI for interacting with Google oauth tokens

# brew "git-credential-oauth" # Git credential helper that authenticates in browser using OAuth
# brew "oauth2_proxy" # Reverse proxy for authenticating users via OAuth 2 providers

