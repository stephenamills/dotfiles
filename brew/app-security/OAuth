# brew "kubelogin" # OpenID Connect authentication plugin for kubectl
# brew "opkssh" # Enables SSH to be used with OpenID Connect
# brew "ory-hydra" # OpenID Certified OAuth 2.0 Server and OpenID Connect Provider
