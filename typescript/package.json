{
  "name": "stephen-project-template",
  "description": "Stephen's project template",
  "version": "0.01",
  "private": true,
  "scripts": {
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:format": "prettier --check \"**/*.{js,jsx,md,ts,tsx}\"",
    "setup-prettier": "npm i -D prettier prettier-plugin-tailwindcss",
    "setup-tailwind": "npm i -D tailwindcss",
    "setup-typescript": "npm i -D typescript @types/node",
    "setup-all": "npm run setup-prettier && npm run setup-tailwind && npm run setup-typescript"
  },
  "devDependencies": {}
}