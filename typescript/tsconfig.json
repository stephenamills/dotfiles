{
  "$schema": "https://json.schemastore.org/tsconfig",
	"include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
	"compilerOptions": {
		"isolatedModules": true,
		"jsx": "react",
		"module": "preserve",
		"target": "ES2022",
		"strict": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"noUncheckedIndexedAccess": true,
		"noEmit": true
	}
}